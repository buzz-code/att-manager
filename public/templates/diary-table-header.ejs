<tr>
    <th> </th>
    <% if (isFilled) { %>
        <th>תז</th>
    <% } %>
    <th>שם / תאריך</th>
    <% days.forEach((day) => {%>
        <% if (isFilled) { %>
            <% if (dates[day.key]) { %>
                <th class="empty_cell" colspan="<%= day.lessons.length %>">
                    <%= dates[day.key] %>
                    <% if (isSubstitute[day.key] == 1) { %>
                        <br/>
                        ממלאת מקום
                    <% } %>
                </th>
            <% } %>
        <% } else { %>
            <th class="empty_cell" colspan="<%= day.lessons.length %>">&nbsp;</th>
        <% } %>
    <% }) %>
</tr>
